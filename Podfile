source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
use_frameworks!
inhibit_all_warnings!

def shared_libs
  pod 'Exteptional', :git => 'https://github.com/serhii-londar/Exteptional.git'
end

target 'awesomeswift' do
  shared_libs
end

target 'awesomeopensource' do
  shared_libs
end
